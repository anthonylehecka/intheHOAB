.page-content-boxed {
  background-color: var(--Content-Background);
  border-radius: 6px;
  /* offset-x | offset-y | blur-radius | spread-radius | color */
  box-shadow: 3px 4px 5px 0px rgba(189, 20, 1, 0.4);
  margin: 0;
  margin-top: 18px;
  margin-left: 18px;
  margin-right: 18px;
  padding: 0;
  padding-top: 6px;
  padding-bottom: 18px;
  padding-left: 24px;
  padding-right: 36px; }

.flex-row-center {
  display: flex;
  justify-content: center;
  margin: 0;
  margin-top: 20px;
  padding: 0; }

.flex-row-left {
  display: flex;
  justify-content: left;
  margin: 0;
  margin-top: 20px;
  padding: 0; }

.flex-row-right {
  display: flex;
  justify-content: right;
  margin: 0;
  margin-top: 20px;
  padding: 0; }

.quote-borders-top-bottom {
  border-top: 1px solid var(--Tawny);
  border-bottom: 1px solid var(--Tawny); }

.quote-border-top {
  border-top: 1px solid var(--Tawny); }

.quote-border-bottom {
  border-bottom: 1px solid var(--Tawny); }

.default-paragraph-text {
  color: var(--Default-Body-Text);
  font-style: normal;
  margin: 0;
  margin-top: 20px;
  padding: 0;
  text-align: justify; }

.bible-quote-text {
  color: var(--Bible-Text);
  font-style: normal;
  margin: 0;
  margin-top: 10px;
  text-align: center; }

.bible-quote-source {
  color: var(--Bible-Source);
  font-style: normal;
  margin: 8px 15px 0px 15px;
  /* Top - Right - Bottom - Left */
  text-align: center; }

.standard-quote-text {
  color: var(--Quote-Text);
  font-style: italic;
  margin: 0;
  margin-top: 10px;
  text-align: center; }

.standard-quote-source {
  color: var(--Quote-Source);
  font-style: normal;
  margin: 8px 15px 0px 15px;
  /* Top - Right - Bottom - Left */
  text-align: center; }

a.topicLink {
  color: var(--Breadcrumb);
  text-decoration: none; }

.zzhoab-siteheader-grid {
  display: flex;
  flex-direction: row;
  background-color: aqua; }

.zzhoab-siteheader-logo {
  width: 33%;
  place-content: center;
  background-color: red;
  height: 150px;
  margin: 0;
  padding: 20px;
  border-top: 20px solid green;
  border-bottom: 3px solid #E2A30A; }

.zzhoab-siteheader-menu {
  width: 67%;
  place-content: space-around;
  vertical-align: middle;
  background-color: #E8BD8D;
  background-color: pink;
  height: 150px;
  margin: 0;
  padding: 0;
  padding-left: 20px;
  border-top: 3px solid #FE7D6A;
  border-left: 2px dotted whitesmoke;
  border-bottom: 3px solid #E2A30A; }

