/* * Heart of All Beings Global Variables */
/* Today is January 12, 2024 */
:root {
  --Brick: #7E2811;
  /* * Body Text */
  --Default-Body-Text: #7E2811;
  /* * Redefines Body Text */
  --Cinnamon-Text: #5C0013;
  /* * Alternate Body Text */
  --Area-Background: #FFFAEB;
  --Content-Background: #F2EDDE;
  /* 5% Darker than Area-Background */
  --Content-Background: #F5F0E1;
  /* 4% Darker than Area-Background */
  --Content-Background: #F7F2E3;
  /* 3% Darker than Area-Background */
  /* * Content Boxes, etc. */
  --Site-Background: #FCF7F7;
  /* * Behind Image */
  --Header-H1-Red: #D62D1A;
  /* * H1 to H6 Gradually Darkens */
  --Header-H2-Red: #BD1401;
  --Header-H3-Red: #A30000;
  --Header-H4-Red: #8A0000;
  --Header-H5-Red: #700000;
  --Header-H6-Red: #570000;
  --Audio-Green: #C4E4B5;
  --Audio-Orange: #FFBB72;
  --Breadcrumb: #484837;
  --Breadcrumb-Hover: #EE5500;
  --Border-Red: #A52A2A;
  --Border-Gold: #E2A30A;
  --Bible-Text: #FF0000;
  --Bible-Source: #FF8C00;
  --Quote-Text: #234F1E;
  --Quote-Source: #FF8C00;
  --Coral: #FE7D6A;
  --Dark-Orange: #FF8C00;
  --Gelasio-Green: #7C795D;
  --Honey: #EC9705;
  --Orange: #ED7014;
  --Pink: #F699CD;
  --Pine: #234F1E;
  --Saddle-Brown: #8B4513;
  --Saffron: #FBB917;
  --Salmon: #FF907F;
  --Sanskrit-Red: #A52A2A;
  --Tangerine: #FA8128;
  --Tawny: #80471C; }

/* * See articles on rem for this trick to make rem to px calculation easy */
/* * Assumption for this trick is that the default px size of text is 16px in all browsers */
html {
  font-size: 62.5%; }

/* * Makes font size in px = to 10px */
body {
  font-size: 1.5rem; }

/* * 10px times 1.5 =15px  */
h1 {
  font-size: 3.6rem; }

/* * =36px */
h2 {
  font-size: 3.2rem; }

/* * =32px */
h3 {
  font-size: 2.8rem; }

/* * =28px */
h4 {
  font-size: 2.4rem; }

/* * =24px */
h5 {
  font-size: 2.2rem; }

/* * =22px */
h6 {
  font-size: 2rem; }

/* * =20px */
ul {
  font-size: 1.1em; }

/* * =11px? */
li {
  font-size: 1.1em; }

/* * =11px? */
body {
  color: var(--Default-Body-Text);
  font-family: "Libre Caslon Text", serif; }

h1 {
  color: var(--Header-H1-Red);
  font-family: "Cabin Condensed", sans-serif;
  margin: 0;
  margin-bottom: 20px;
  padding: 0; }

h2 {
  color: var(--Header-H2-Red);
  font-family: "Cabin Condensed", sans-serif; }

h2.pagename-Gelasio-Green {
  color: var(--Gelasio-Green);
  font-family: "Gelasio", serif;
  font-size: 3rem;
  font-weight: normal;
  text-decoration: underline; }

h3 {
  color: var(--Header-H3-Red);
  font-family: "Cabin Condensed", sans-serif; }

h4 {
  color: var(--Header-H4-Red);
  font-family: "Cabin Condensed", sans-serif; }

h5 {
  color: var(--Header-H5-Red);
  font-family: "Cabin Condensed", sans-serif; }

h6 {
  color: var(--Header-H6-Red);
  font-family: "Cabin Condensed", sans-serif; }

.Sanskrit-Text {
  font-family: "Sanskrit2003", serif;
  font-size: 5.5rem;
  color: var(--Sanskrit-Red);
  margin-top: 6px;
  margin-bottom: 6px; }

.Sanskrit-Text-Translation {
  font-family: "Courgette", serif;
  font-size: 2.8rem;
  color: var(--Saddle-Brown);
  margin-top: 6px;
  margin-bottom: 6px; }

/* * The Breadcrumb Line is navyby-url-breadcrumb on the left */
/* * and navby-prev-next-post-breadcrumb on the right */
.breadcrumb-line-flex-container {
  display: flex;
  flex-direction: row; }

.breadcrumb-line-flex-left {
  width: 50%; }

.breadcrumb-line-flex-right {
  width: 50%; }

/* * BreadCrumbs (Site Tree Navigation by URL) */
div.navby-url-breadcrumbs {
  width: 100%;
  border-style: none;
  padding: 0; }

.navby-url-breadcrumbs ul {
  list-style: none;
  margin: 0px;
  padding-left: 0px;
  font-size: 90%; }

.navby-url-breadcrumbs ul li {
  display: inline-block;
  margin-right: 0px; }

.navby-url-breadcrumbs ul li.url-separator {
  font-size: 160%; }

.navby-url-breadcrumbs ul li.prior-page-name {
  color: var(--Breadcrumb-Hover); }

.navby-url-breadcrumbs ul li a {
  color: var(--Breadcrumb);
  text-decoration: none; }

.navby-url-breadcrumbs ul li a:hover {
  color: var(--Breadcrumb-Hover);
  text-decoration: none; }

/* * BreadCrumbs (Posts Navigation by Prev|Current|Next Post) */
div.navby-prev-next-post-breadcrumbs {
  width: 100%;
  border-style: none;
  padding: 0;
  text-align: right; }

.navby-prev-next-post-breadcrumbs ul {
  list-style: none;
  margin: 0px;
  padding-left: 0px;
  font-size: 90%; }

.navby-prev-next-post-breadcrumbs ul li {
  display: inline-block;
  margin-right: 0px; }

.navby-prev-next-post-breadcrumbs ul li.post-separator {
  font-size: 160%; }

.navby-prev-next-post-breadcrumbs ul li.thispage-gq-name {
  color: var(--Breadcrumb-Hover); }

.navby-prev-next-post-breadcrumbs ul li a {
  color: var(--Breadcrumb);
  text-decoration: none; }

.navby-prev-next-post-breadcrumbs ul li a:hover {
  color: var(--Breadcrumb-Hover);
  text-decoration: none; }

/* * Spacers (Empty Space) */
/* ! N.B. The 'Hot Pink' Background should never show as these spacers */
/* !          should be applied to empty divs only. Otherwise even a simple */
/* !          &nbsp; will make the spacer too large (vertically) */
.spacer-1rem {
  margin: 0;
  padding: 0;
  margin-top: 1rem;
  background-color: #FF10F0; }

.spacer-2rem {
  margin: 0;
  padding: 0;
  margin-top: 2rem;
  background-color: #FF10F0; }

.spacer-3rem {
  margin: 0;
  padding: 0;
  margin-top: 3rem;
  background-color: #FF10F0; }

/* * Spacer (<HR> Like */
.thinline {
  height: 0;
  margin: 0;
  margin-top: 30px;
  margin-bottom: 30px;
  margin-left: auto;
  margin-right: auto;
  background: transparent;
  border-top: 1px solid var(--Salmon);
  border-bottom: 1px solid darkgray;
  width: 90%;
  text-align: center; }

/* Default Layout is 3 Columns */
.css-pagegrid-homepage {
  display: grid;
  grid-template-columns: 1% auto 30%;
  grid-template-areas: "gtaHP-page-header  gtaHP-page-header  gtaHP-page-header" "gtaHP-left-sidebar gtaHP-page-content gtaHP-right-sidebar" "gtaHP-page-footer  gtaHP-page-footer  gtaHP-page-footer"; }

